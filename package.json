{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with Vanilla and nodejs",
  "author": "kth",
  "license": "MIT",
  "type": "module",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-syntax-import-meta"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "autoprefixer": "^9.8.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^3.22.0",
    "mongoose": "^5.9.15",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss-cli": "^7.1.1",
    "pug": "^2.0.4"
  },
  "scripts": {
    "dev:server": "nodemon --exec node src/init.js --delay 2  --ignore 'scss' --ignore 'static' ",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack --config ./webpack.config.cjs -w",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack --config ./webpack.config.cjs",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.cjs'",
    "copyAll": "xcopy .\\src\\static .\\build\\static\\ && xcopy .\\src\\views .\\build\\views\\",
    "build": "yarn build:server && yarn build:assets && yarn copyAll",
    "prebuild": "rm -rf build",
    "start": "node build/init.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-syntax-import-meta": "^7.10.1",
    "@babel/preset-env": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "nodemon": "^2.0.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "prettier-eslint": "^9.0.1",
    "sass-loader": "^8.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
